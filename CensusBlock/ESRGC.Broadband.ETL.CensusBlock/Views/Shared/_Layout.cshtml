<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Maryland Broadband - ETL Tool</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/customCSS/style", "~/Content/bootstrap/css/style")
    @Scripts.Render("~/bundles/modernizr", "~/bundles/jquery") 
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap.js")" type="text/javascript"></script>
    
</head>
<body>
    <div id="body" class="container-fluid">
        <div class="navbar ">
            <div class="navbar-inner">
                <div class="container">
                    <a class="brand" href="#">ETL Tool</a>
                    <ul class="nav">
                        <li class=""><a href="@Url.Action("Index", "Home")"><i class="icon-home"></i> Home</a></li>
                        <li><a href="#"><i class="icon-question-sign"></i> Help</a></li>
                        <li><a href="#"><i class="icon-user"></i> Contact</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <header class="page-header">
            @RenderSection("header", required: false)
        </header>
        @RenderSection("featured", required: false)
        <section>
            @RenderBody()
        </section>
        <hr />
        @if (ViewBag.Title != "Home") {
        <footer>
            <!--Put something in here for footer-->
            <div class="row-fluid">
                <ul class="thumbnails">
                    <li class="span1 offset3"><img src="@Url.Content("~/Content/images/recoveryGov.jpg")" /></li>
                    <li class="span1"><img src="@Url.Content("~/Content/images/MDBC.jpg")" /></li>
                    <li class="span1"><img src="@Url.Content("~/Content/images/ntia_logo.png")" /></li>
                    <li class="span1"><img src="@Url.Content("~/Content/images/MDlogo.gif")" /></li>
                    <li class="span1"><img src="@Url.Content("~/Content/images/SuLogo.jpg")" /></li>
                    <li class="span1"><img src="@Url.Content("~/Content/images/towson.gif")" /></li>
                </ul>
            </div>
        </footer>
        }
    </div>   
    @RenderSection("scripts", required: false)
</body>
</html>
