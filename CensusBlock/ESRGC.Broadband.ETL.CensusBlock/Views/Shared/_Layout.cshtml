<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Maryland Broadband - ETL Tool</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/customCSS/style", "~/Content/bootstrap/css/style")
    @Scripts.Render("~/bundles/modernizr")
    
</head>
<body>
    <div id="body" class="container-fluid">
        <div class="navbar ">
            <div class="navbar-inner">
                <div class="container">
                    <a class="brand" href="#">ETL Tool</a>
                    <ul class="nav">
                        <li class=""><a href="@Url.Action("Index", "Home")"><i class="icon-home"></i> Home</a></li>
                        <li><a href="#"><i class="icon-question-sign"></i> Help</a></li>
                        <li><a href="#"><i class="icon-user"></i> Contact</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <header class="page-header">
            @RenderSection("header", required: false)
        </header>
        @RenderSection("featured", required: false)
        <section>
            @RenderBody()
        </section>
        <footer>
            <!--Put something in here for footer-->
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap.js")" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
