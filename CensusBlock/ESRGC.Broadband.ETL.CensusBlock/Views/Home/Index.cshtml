    <div class="row-fluid">
        <div class="span8">
            <div class="hero-unit">
                <h2>Maryland Broadband</h2>
                <p>ETL Data Upload Tool</p>
                <small>An easy way to upload broadband service data for Census Block.</small>
            </div>
        </div>
        <div class="span4">
            <div class="row-fluid">
                <div class="span12">
                    <h5>Partners</h5>
                </div>
                <div class="span12">
                    <ul class="thumbnails">
                        <li class="span3">
                            <a target="_blank" href="http://www.recovery.gov/Pages/default.aspx">
                                <img src="@Url.Content("~/Content/images/recoveryGov.jpg")" alt=""/>
                            </a>
                        </li>
                        <li class="span3">
                            <a target="_blank" href="http://www.mdbc.us/">
                                <img src="@Url.Content("~/Content/images/MDBC.jpg")" alt=""/>
                            </a>
                        </li>
                        <li class="span3">
                            <a target="_blank" href="http://www.ntia.doc.gov/">
                                <img src="@Url.Content("~/Content/images/ntia_logo.png")" alt=""/>
                            </a>
                        </li>
                    </ul>
                    <ul class="thumbnails">
                        <li class="span3">
                            <a target="_blank" href="http://www.gov.state.md.us/">
                                <img src="@Url.Content("~/Content/images/MDlogo.gif")" alt=""/>
                            </a>
                        </li>
                        <li class="span3">
                            <a target="_blank" href="http://www.esrgc.org/">
                                <img src="@Url.Content("~/Content/images/SuLogo.jpg")" alt=""/>
